{
  "horizon": {
    "hostname": "${HZN_HOSTNAME}",
    "namespace": "${HZN_NAMESPACE}",
    "network": {
      "name": "${HZN_NETWORK}",
      "driver": "${HZN_NETWORK_DRIVER}"
    },
    "identity": {
      "comp": "${SOMECOMP}",
      "user": "${SOMEUSER}",
      "name": "${SOMENAME}",
      "subj": "${SOMESUBJ}"
    },
    "root": "${EXCHANGE_ROOT}"
  },
  "services": {
    "agbot": {
      "name": "agbot",
      "image": "openhorizon/amd64_agbot",
      "tag": "${AGBOT_TAG}",
      "stable": "2.24.18",
      "port": 3091,
      "listen": "http",
      "bot": {
        "name": "agbot1",
        "token": "agbot1pw"
      }
    },
    "css": {
      "name": "css-api",
      "image": "openhorizon/amd64_cloud-sync-service",
      "tag": "${CSS_TAG}",
      "stable": "1.0.18",
      "port": 9443,
      "transport": "http",
      "listen": "http"
    },
    "exchange": {
      "name": "exchange-api",
      "image": "openhorizon/amd64_exchange-api",
      "tag": "${EXCHANGE_TAG}",
      "stable": "2.12.3",
      "version": "v1",
      "port": ${EXCHANGE_PORT},
      "listen": "http",
      "root": "root",
      "log": "WARN",
      "password": "${EXCHANGE_PASSWORD}",
      "encoded": "${EXCHANGE_PASSWORD_ENCODED}",
      "org": "${EXCHANGE_ORG}",
      "admin": {
        "username": "${EXCHANGE_ADMIN_USERNAME}",
        "password": "${EXCHANGE_ADMIN_PASSWORD}"
      }
    },
    "cssdb": {
      "name": "css-db",
      "image": "mongo",
      "tag": "4.0.6",
      "database": "css-db",
      "volume": "cssdb"
    },
    "exchangedb": {
      "name": "exchange-db",
      "image": "postgres",
      "tag": "9",
      "port": 5432,
      "user": "admin",
      "password": "",
      "database": "exchange",
      "volume": "exchangedb"
    }
  }
}
