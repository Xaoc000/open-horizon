{
  "global": [],
  "services": [
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.motion2mqtt",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "HZN_EXCHANGE_APIKEY": "${HZN_EXCHANGE_APIKEY}",
        "MOTION_TIMEZONE": "America/Los_Angeles",
        "MOTION_GROUP": "motion",
        "MOTION_CLIENT": "",
        "MOTION_POST_PICTURES": "center",
        "MOTION_LOG_LEVEL": 6,
        "MOTION_LOG_TYPE": "all",
        "MQTT_HOST": "mqtt",
        "MQTT_USERNAME": "username",
        "MQTT_PASSWORD": "password",
        "MQTT_PORT": 1883,
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.startup",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "STARTUP_HOST_USER": "${USER}",
        "STARTUP_PERIOD": 600,
        "STARTUP_SYNC_PERIOD": 600,
        "STARTUP_KAFKA_APIKEY": "",
        "STARTUP_KAFKA_TOPIC": "startup",
        "LOGTO": "",
        "LOG_LEVEL": "info",
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.cpu",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "CPU_PERIOD": 60,
        "LOG_LEVEL": "info",
        "LOGTO": "",
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.hal",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "HAL_PERIOD": 43200,
        "LOG_LEVEL": "info",
        "LOGTO": "",
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.wan",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "WAN_PERIOD": 14400,
        "LOG_LEVEL": "info",
        "LOGTO": "",
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.mqtt",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "MQTT_PORT": 1883,
        "MQTT_USERNAME": "username",
        "MQTT_PASSWORD": "password",
        "LOG_LEVEL": "info"
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.mqtt2mqtt",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "MQTT_USERNAME": "username",
        "MQTT_PASSWORD": "password",
        "MQTT_HOST": "mqtt",
        "MQTT_PORT": 1883,
        "MQTT2MQTT_SUBSCRIBE": "+/+/+/event/end",
        "MQTT2MQTT_PUBLISH": "",
        "MQTT2MQTT_HOST": "mqtt.dcmartin.com",
        "MQTT2MQTT_PORT": "1883",
        "MQTT2MQTT_USERNAME": "username",
        "MQTT2MQTT_PASSWORD": "password",
        "MQTT2MQTT_PERIOD": "30",
        "LOG_LEVEL": "info",
        "LOGTO": "",
        "DEBUG": false
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.jetson-yolo4motion",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "MQTT_HOST": "mqtt",
        "MQTT_USERNAME": "username",
        "MQTT_PASSWORD": "password",
        "MQTT_PORT": 1883,
        "MOTION_GROUP": "motion",
        "MOTION_CLIENT": "",
        "YOLO4MOTION_CAMERA": "+",
        "YOLO_CONFIG": "tiny",
        "YOLO_ENTITY": "all",
        "YOLO_SCALE": "none",
        "LOG_LEVEL": "info"
      }
    }
  ]
}
